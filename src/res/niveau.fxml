<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <AnchorPane fx:id="pane" focusTraversable="true" onKeyPressed="#keyPressed" onKeyReleased="#keyReleased" onMouseClicked="#mouseClicked" onScroll="#zoom" prefHeight="200.0" prefWidth="200.0" />
      
   </center>
   <top>
      <ImageView fitHeight="61.0" fitWidth="1294.0" BorderPane.alignment="CENTER">
         <image>
            <Image url="@titleeditor.png" />
         </image>
      </ImageView>
   </top>
   <bottom>
      <HBox alignment="CENTER" stylesheets="@test.css" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="retour" mnemonicParsing="false" onAction="#retour" text="Retour" />
            <Button fx:id="sauve" mnemonicParsing="false" onAction="#sauve" text="Sauvegarder" />
            <Button fx:id="erase" mnemonicParsing="false" onAction="#erase" text="Effacer" />
            <ChoiceBox fx:id="choice" prefHeight="40.0" prefWidth="150.0" />
            <Separator prefWidth="200.0" />
            <ColorPicker fx:id="color" prefHeight="41.0" prefWidth="81.0" />
            <Separator prefHeight="1.0" prefWidth="61.0" />
            <Slider fx:id="masse" />
            <Label text="Masse" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
